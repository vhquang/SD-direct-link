{
  "manifest_version": 2,

  "name": "SlickDeals no-redirect link",
  "short_name": "SD skip",
  "description": "Open original deal's link in new tab, without going 3rd party websites",
  "version": "0.0.2",

  "permissions": [
    "contextMenus",
    "*://slickdeal.net/*"
  ],

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "background": {
    "scripts": [
      "convert.js"
    ]
  }
}
